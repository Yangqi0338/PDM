    <!-- ***********************自定义方法区 不替换的区域【other_start】***************************************** -->
    <select id="findByFId" resultType="com.base.sbc.module.common.vo.AttachmentVo">
        select * from t_attachment a ,t_upload_file f
        where a.file_id=f.id
        and a.f_id=#{fId}

    </select>

