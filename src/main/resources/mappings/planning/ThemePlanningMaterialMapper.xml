    <!-- ***********************自定义方法区 不替换的区域【other_start】***************************************** -->
    <select id="getByThemePlanningId" resultType="com.base.sbc.module.planning.vo.ThemePlanningMaterialVO">
        SELECT id,
        STATUS,
        remarks,
        material_image,
        material_classif_id,
        material_classif,
        material_name,
        material_id,
        tag,
        tag_code,
        tag_color
        FROM t_theme_planning_material
        <where>
            del_flag = '0'
            and theme_planning_id = #{themePlanningId}
        </where>
    </select>

    <!-- ***********************自定义方法区 不替换的区域【other_end】******************************************** -->
</mapper>

