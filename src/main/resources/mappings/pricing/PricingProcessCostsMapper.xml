    <!-- ***********************自定义方法区 不替换的区域【other_start】***************************************** -->
    <select id="getByPricingCode" resultType="com.base.sbc.module.pricing.vo.PricingProcessCostsVO">
        SELECT
        id,
        pricing_code,
        sort,
        part,
        process_name,
        process_level,
        finally_node,
        gst_process_price,
        magnification multiple,
        ie_process_price,
        process_date,
        max_proce_price,
        currency,
        quotation_price,
        quotation_price_currency,
        process_price
        FROM
        t_pricing_process_costs
        <where>
            del_flag = 0
            and pricing_code = #{pricingCode}
            and company_code = #{userCompany}
        </where>
    </select>

    <!-- ***********************自定义方法区 不替换的区域【other_end】******************************************** -->
</mapper>