<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.base.sbc.module.sample.mapper.PreProductionSampleTaskMapper">
    <!--
        产前样-任务
        创建人：your name
        邮箱：your email
        创建时间：2023-7-18 11:04:08
        版本号：1.0
      -->

    <!-- ***********************自定义方法区 不替换的区域【other_start】***************************************** -->

    <select id="taskList" resultType="com.base.sbc.module.sample.vo.PreProductionSampleTaskListVo">
        select t.*,s.style_no,s.style_pic from t_pre_production_sample s,t_pre_production_sample_task t
        where s.id=t.pre_production_sample_id and sample_complete_flag='0'
        and s.del_flag='0'
        and t.del_flag='0'
        <if test="ew.customSqlSegment != null and ew.customSqlSegment != ''">
            ${ ew.customSqlSegment.startsWith("WHERE")?ew.customSqlSegment.replace("WHERE","AND "):ew.customSqlSegment}
        </if>

    </select>

    <!-- ***********************自定义方法区 不替换的区域【other_end】******************************************** -->
</mapper>